<view><scroll-view class="bg-white nav" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{List}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item ',index==TabCur?'text-brown cur':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</view></block></scroll-view><scroll-view class="bg-white nav" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{subList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item ',index==subTabCur?'text-brown cur':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['subtabSelect',['$event']]]]]}}" bindtap="__e">{{item}}</view></block></scroll-view><view class="cu-list grid col-3 solid-top"><view class="cu-item"><text>名称</text></view><view class="cu-item"><text>状态</text></view><view class="cu-item"><text>操作</text></view></view><block wx:for="{{promotiondata}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-list grid col-3 solid-top"><view class="cu-item"><view class="_p">{{item.name}}</view></view><view class="cu-item"><view class="{{['_p',item.code=='1'?'text-green':''||item.code=='3'?'text-blue':'']}}">{{item.status}}</view></view><view class="cu-item"><view class="text-brown _p">{{item.operate}}</view></view></view></block></view>